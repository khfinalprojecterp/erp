<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE configuration PUBLIC
"-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>

 	<typeAliases>
		
		<typeAlias type="com.kh.erp.m_storage.model.vo.M_storage"
		           alias="M_storage" />
		<typeAlias type="com.kh.erp.common.typeHandler.StringArrayTypeHandler"
				   alias="strArrTypeHandler" />
	</typeAliases> 
	
	
	
	<!-- 어느 부분의 typeHandler를 적용할 것인지 패키지 영역을 설정한다. -->
	<typeHandlers>
		<package name="com.kh.erp.common.typeHandler"/>
	</typeHandlers> 
</configuration>










